    <rules>
        <fullName>CSAT Survey - Belgium</fullName>
        <actions>
            <name>Email_alert_to_Belgium_Contact_if_customer_survey_become_true</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Customer_Survey_Result_Survey_email_notification</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Customer_Survey_Result__c.Survey_Status__c</field>
            <operation>notEqual</operation>
            <value>Responded</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.BillingCountry</field>
            <operation>equals</operation>
            <value>Belgium</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Contact_Language__c</field>
            <operation>equals</operation>
            <value>Dutch</value>
        </criteriaItems>
        <criteriaItems>
            <field>Customer_Survey_Result__c.Survey_Result_Created_Date__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </criteriaItems>
        <description>Belgium3: CSAT Belgium Survey</description>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_to_Belgium_Contact_if_customer_survey_become_true</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Survey_Status_after_10days</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Customer_Survey_Result_Survey_email_notification</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Customer_Survey_Result__c.CreatedDate</offsetFromField>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_to_Belgium_Contact_if_customer_survey_become_true</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Survey_Status_after_20days</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Customer_Survey_Result_Survey_email_notification</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Customer_Survey_Result__c.CreatedDate</offsetFromField>
            <timeLength>20</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_to_Belgium_Contact_if_customer_survey_become_true</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Survey_Status_after_25_days</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Customer_Survey_Result_Survey_email_notification</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Customer_Survey_Result__c.CreatedDate</offsetFromField>
            <timeLength>25</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>CSAT Survey - France</fullName>
        <actions>
            <name>Email_alert_to_French2_Contact_if_customer_survey_become_true</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Customer_Survey_Result_Survey_email_notification</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <description>France3: CSAT French Survey</description>
        <formula>(Partner_Account__r.Name &lt;&gt; &apos;TENESOL VDP&apos;)  
&amp;&amp;  
( ISPICKVAL(Survey_Status__c, &apos;Responded&apos;) )
 &amp;&amp; 
( Survey_Result_Created_Date__c &lt; TODAY() )
 &amp;&amp;
(( Partner_Account__r.BillingCountry  = &apos;France&apos;)   ||                                                                        ((ISPICKVAL(Partner_Account__r.Contact_Language__c, &apos;French&apos;))   
&amp;&amp;  
( Partner_Account__r.BillingCountry  = &apos;Belgium&apos;)))</formula>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Survey_Status_after_25_days</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Customer_Survey_Result__c.CreatedDate</offsetFromField>
            <timeLength>25</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_to_French2_Contact_if_customer_survey_become_true</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Survey_Status_after_10days</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Customer_Survey_Result_Survey_email_notification</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Customer_Survey_Result__c.CreatedDate</offsetFromField>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_to_French2_Contact_if_customer_survey_become_true</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Survey_Status_after_20days</name>
