<apex:page showHeader="false" sidebar="false" standardController="Contact" extensions="TerminateContact">
    <c:SPCommunityAppResources_CSS namespaceOnly="true"/>
    <c:CommunitiesMainNavComponent activeTab="Accounts" />
    <body class="detail-container">
        <apex:pagemessages />
        <apex:form >
    		<apex:pageBlock >
      			<apex:pageBlockSection >
                	<apex:outputLabel value="{!message}" />
            	</apex:pageBlockSection>
            	<apex:pageBlockButtons >
                	<apex:commandButton value="Proceed" action="{!Proceed}"/>
                	<apex:commandButton value="Cancel" action="{!Cancel}"/>
           	 	</apex:pageBlockButtons>
        	</apex:pageBlock>
      	</apex:form>
    </body>
    <c:SPCommunityAppResources_JS />
</apex:page>