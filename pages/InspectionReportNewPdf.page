<script>

    window.onload = CallFunctionsOnPageLoad ;
    
    function CallFunctionsOnPageLoad() {
        CheckScoreFillColor();
    }
    
    function CheckScoreFillColor() {
 
    var scoreRes = document.getElementById('inputResult').value;
    var strResult = scoreRes.indexOf('PASS');
    if( strResult >= 0) {
        document.getElementById('inputScore').style.color = "Green";
        document.getElementById('inputResult').style.color = "Green";
    }
    else{
        document.getElementById('inputScore').style.color = "Red";
        document.getElementById('inputResult').style.color = "Red";
    }
 }
    
</script>

