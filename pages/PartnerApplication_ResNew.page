    <script type="text/javascript">
    $j = jQuery.noConflict();
        $j(document).ready(function(){
            var icons = {
                header: "defaultIcon",
                activeHeader: "selectedIcon"
            };

           $j("#accordion").accordion({heightStyle: "content", icons: icons});

           var progressbarValue = 0;
           $j( "#progressbar" ).progressbar({
            value: progressbarValue
           });
           $j(".datepicker").datepicker();

           $j("div.disabled input, div.disabled select, div.disabled textarea").attr("disabled", true);
           $j("div.disabled").addClass("grayed");
           $j("div.disabled .buttonSection").hide();

           $j("#validateFields").bind("click", function(event){
              var companyName = $j(".companyName").val();

              if(companyName == "")
              {
                  $j(".errors").text("Please fill out all required fields before attempting to continue!");
              }
              else
              {
                  $j(".errors").text("");
                  if($j(this).parent().parent().next().next().hasClass("disabled"))
                  {
                      $j(this).parent().parent().next().next().removeClass("disabled");
                      $j(this).parent().parent().next().next().find("input, select, textarea").removeAttr("disabled");
                      $j(this).parent().parent().next().next().removeClass("grayed");
                      $j(this).parent().parent().next().next().children(".buttonSection").show();
                  }
                  increaseProgressBar($j(this));
                  if(!$j(this).parent().parent().prev("h3").hasClass("completed"))
                  {
                        $j(this).parent().parent().prev("h3").addClass("completed");
                  }
                  $j(this).parent().parent().next("h3").trigger("click");
              }
              event.preventDefault();
           });

           $j(".back").bind("click", function(event){
              $j(this).parent().parent().prev().prev().prev("h3").trigger("click");
              event.preventDefault();
           });

           $j(".continue").bind("click", function(event){
              if($j(this).parent().parent().next().next().hasClass("disabled"))
              {
                      $j(this).parent().parent().next().next().removeClass("disabled");
                      $j(this).parent().parent().next().next().find("input, select, textarea").removeAttr("disabled");
                      $j(this).parent().parent().next().next().removeClass("grayed");
                      $j(this).parent().parent().next().next().children(".buttonSection").show();
              }
              increaseProgressBar($j(this));
              if(!$j(this).parent().parent().prev("h3").hasClass("completed"))
              {
                    $j(this).parent().parent().prev("h3").addClass("completed");
              }
              $j(this).parent().parent().next("h3").trigger("click");
              event.preventDefault();
           });

           var increaseProgressBar = function(element){
           if(element.hasClass("firstTime"))
           {
                element.removeClass("firstTime");
                progressbarValue += 10;
                $j("#progressbarState").text(progressbarValue);
                $j( "#progressbar" ).progressbar("value", progressbarValue );
           }
           }
        });
    </script>
