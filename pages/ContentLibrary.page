<apex:page controller="DocumentLibraryController" sidebar="true" tabStyle="Content__c" showHeader="true" id="page1">
<script>

function highlightTab(){
    var tableId = document.getElementById("tabBar");
    var rowId  = tableId.tBodies[0].rows[0];
    var cellIds = rowId.getElementsByTagName('td');
    for(var i=0;i<cellIds.length;i++){
        var cellId1 = cellIds[i]; 
        var DivTag = cellId1.getElementsByTagName('div');
        var divT = DivTag[0];
        var aTag = divT.getElementsByTagName('a');
        var aT = aTag[0];
        if(aT.href.indexOf('01rT00000004boZ')>0 || aT.href.indexOf('01r80000000Puys')>0){
            if(cellId1!=null){
                if(cellId1.getAttribute('className')!='currentTab primaryPalette' || cellId1.getAttribute('class')!='currentTab primaryPalette'){
                    cellId1.setAttribute('className','currentTab primaryPalette');
                    cellId1.setAttribute('class','currentTab primaryPalette');
                }
            }
            break;
        }
    }
}

window.onload = function(){ highlightTab(); };
    
function ShowHideArchive(){
    var ctrl = document.getElementById("page1:block1:form1:chkArchive");
    var bttn = document.getElementById('page1:block1:form1:btnSearch');
    bttn.disabled = true;
    
    if(ctrl != undefined){           
        ShowHideArchive1(ctrl.checked);
    }
}

function enableSearch(){
    var bttn = document.getElementById('page1:block1:form1:btnSearch');
    
    bttn.disabled = false;
}

function MoveToSelPage(strClick){       
       MoveToPage(strClick);
}
function deleteCurrent(Id) {
    if(confirm("Are you sure?")) {
       removeContent(Id);      
    }
    else
      return false;     
}
</script>
