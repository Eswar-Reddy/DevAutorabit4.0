                                <apex:column headerValue="Action" headerClass="underline">
                                    <apex:commandLink action="{!deleteInvDoc}" styleClass="secondary-link" rendered="{!idoc.canDelete && !idoc.isLegacy && (partnerRole == null || (partnerRole != null && idoc.invDoc.Document_Owner__c == partnerRole))}" style="float:left;"> <i class="fa fa-trash-o" aria-hidden="true"></i>
                                        Delete
                                        <apex:param name="deleteDocId" value="{!idoc.attId}" assignTo="{!deleteDocId}"/>
                                        <apex:param name="currTab" value="2" assignTo="{!currTab}"/>                                        
                                    </apex:commandLink>

                                    <apex:inputFile styleClass="file-input" value="{!idoc.attBody}" filename="{!idoc.attName}" rendered="{!!idoc.canDelete && !idoc.isLegacy && ISBLANK(idoc.attId) && (partnerRole == null || (partnerRole != null && idoc.invDoc.Document_Owner__c == partnerRole))}"/>
                                    <apex:outputText value="Attachment on: {!idoc.invDoc.Location__c}" rendered="{!idoc.isLegacy}"/>

                                </apex:column>

                                <apex:column headerValue="Date Submitted" headerClass="underline">
                                    <apex:outputField value="{!idoc.invDoc.Document_Submitted__c}" rendered="{!idoc.isComp}"/>
                                </apex:column>

                                <apex:column headerValue="Response Date" headerClass="underline">
                                    <apex:outputField value="{!idoc.invDoc.Response_Date__c}" rendered="{!idoc.isComp}"/>
                                </apex:column>

                                <apex:column headerValue="Partner Comments" headerClass="underline">
                                    <apex:inputText value="{!idoc.invDoc.Partner_Comments__c}" rendered="{!idoc.isComp}" disabled="{!partnerRole != null && idoc.invDoc.Document_Owner__c != partnerRole}" style="position:relative; top:-5px;" maxlength="255"/>
                                </apex:column>

                                <apex:column headerValue="Approver Notes" headerClass="underline">
                                    <apex:outputText value="{!idoc.invDoc.Notes_to_Dealer__c}" rendered="{!idoc.isComp}"/>
                                </apex:column>

                            </apex:dataTable>
                        </div>
                    </div>
