<apex:page id="pgNewPartnerScorecard" standardController="Partner_Application__c" extensions="NewPartnerScorecardController" sidebar="false" showHeader="false">
<script>
 function showPopUp(){
      if(confirm("Die Scorecard wird automatisch vervollständigte. Bist du sicher?")) {
            return true;
      }
      else {
            return false;
      }
  }
 function openPDF(){
     window.open('/apex/New_Partner_Scorecard_Pdf_ge?id='+'{!id}','_blank','width=700,height=650');;
 }
 function IsNumeric(sText)
    {
        var ValidChars = "0123456789.";
        var IsNumber=true;
        var Char;
        for (i = 0; i < sText.length && IsNumber == true; i++) 
          { 
          Char = sText.charAt(i); 
          if (ValidChars.indexOf(Char) == -1) 
             {
             IsNumber = false;
             }
          }
       return IsNumber;
   }

   function validateValue(elem, val){
        if(IsNumeric(elem.value)){
            if(elem.value > val){
                alert('Wert muss kleiner als oder gleich '+val);
                elem.value='0';
                elem.focus();
            }
        }else{
            elem.value='0';
            return;
        
        }
    }
 </script>
