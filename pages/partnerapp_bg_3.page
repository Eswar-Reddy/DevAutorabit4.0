        <!--  Misc End -->
    </apex:pageBlock>
    </apex:form>
    <script>
        function removeWarningMsg(){
               try{                
                    var ctrl = document.getElementById("page1:form1");
                    var pTags = document.getElementById("page1:form1").getElementsByTagName('div');
                    for(i=0;pTags != null && i < pTags.length; i++ ){
                        var pHTML =pTags[i].innerHTML;
                        if(pHTML != null && pHTML.indexOf('Warning:') >=0){
                            pTags[i].innerHTML='&nbsp;';
                            pTags[i].className='&nbsp;';
                        }   
                    }
                                    
                }catch(e){}
            }
            removeWarningMsg();
            
     </script>
</apex:page>