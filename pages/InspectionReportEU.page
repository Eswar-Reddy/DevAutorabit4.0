function calculateArrayPower(){
    moduleTypeQuantity1 = document.getElementById('pg:frm:pb:inputQuantitySPR1');
    moduleTypeQuantity2 = document.getElementById('pg:frm:pb:inputQuantitySPR2');
    
    if (moduleTypeQuantity1.value.replace(/[0-9]/g, '') != '') {
       //.replace(/[0-9]/g, '').replace(/\./, '')
      moduleTypeQuantity1.focus();
    } else if (moduleTypeQuantity2.value.replace(/[0-9]/g, '') != '') {
      moduleTypeQuantity2.focus();
    }
    
    if ((moduleTypeQuantity2.value.replace(/[0-9]/g, '') != '')
        || (moduleTypeQuantity1.value.replace(/[0-9]/g, '') != '')) {
      alert('You must enter a Integer value for Quanity');
      return;
    }
    
    moduleType1 = document.getElementById('pg:frm:pb:inputSPR1').value ;
    moduleType1  = moduleType1.substring(0,3);
    moduleType2 = document.getElementById('pg:frm:pb:inputSPR2').value ;
    moduleType2  = moduleType2.substring(0,3);
    
    ArrayPower = document.getElementById('pg:frm:pb:inputArrayPower');
    ArrayPower.value  = (moduleType1*moduleTypeQuantity1.value) + (moduleType2*moduleTypeQuantity2.value) ;
    
    if(isNaN(ArrayPower.value))
      ArrayPower.value = '' ;
  }


 
  //Q24, Q 25 Event ...
  function calculateArrayDC(){
    ArrayVoltage1 = document.getElementById('pg:frm:pb:inputArrayVoltage1').value ;
    ArrayVoltage2 = document.getElementById('pg:frm:pb:inputArrayVoltage2').value ;
    ArrayVoltage3 = document.getElementById('pg:frm:pb:inputArrayVoltage3').value ;
    ArrayVoltage4 = document.getElementById('pg:frm:pb:inputArrayVoltage4').value ;
    
    ArrayCurrent1 =  document.getElementById('pg:frm:pb:inputArrayCurrent1').value ;
    ArrayCurrent2 = document.getElementById('pg:frm:pb:inputArrayCurrent2').value ;
    ArrayCurrent3 = document.getElementById('pg:frm:pb:inputArrayCurrent3').value ;
    ArrayCurrent4 = document.getElementById('pg:frm:pb:inputArrayCurrent4').value ;
    
    ArrayDC1 =  document.getElementById('pg:frm:pb:inputArrayDCPower1') ;
    ArrayDC2 =  document.getElementById('pg:frm:pb:inputArrayDCPower2') ;
    ArrayDC3 =  document.getElementById('pg:frm:pb:inputArrayDCPower3') ;
    ArrayDC4 =  document.getElementById('pg:frm:pb:inputArrayDCPower4') ;
  
    
    ArrayDC1.value =  ArrayVoltage1*ArrayCurrent1;
    ArrayDC2.value =  ArrayVoltage2*ArrayCurrent2;
    ArrayDC3.value =  ArrayVoltage3*ArrayCurrent3;
    ArrayDC4.value =  ArrayVoltage4*ArrayCurrent4;
    
    
    if(isNaN(ArrayDC1.value))
      ArrayDC1.value = '' ;
      
    if(isNaN(ArrayDC2.value ))
      ArrayDC2.value = '' ;
    
    if(isNaN(ArrayDC3.value ))
      ArrayDC3.value = '' ;
    
    if(isNaN(ArrayDC4.value ))
      ArrayDC4.value = '' ;
  }
  
