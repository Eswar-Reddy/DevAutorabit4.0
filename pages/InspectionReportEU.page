function findPosX (obj) {
      var curleft = 0;
      if (obj.offsetParent)
        while(true) {
          curleft += obj.offsetLeft;
          if (!obj.offsetParent)
            break;
          obj = obj.offsetParent;
        }
      else if (obj.x)
        curleft += obj.x;                
      return curleft;
     
 }
    
function findPosY (obj) {
      var curtop = 0;
      if (obj.offsetParent)
        while(true) {
          curtop += obj.offsetTop;
          if (!obj.offsetParent)
            break;
          obj = obj.offsetParent;
        }
      else if (obj.y)
        curtop += obj.y;     
      return curtop;
      
}

