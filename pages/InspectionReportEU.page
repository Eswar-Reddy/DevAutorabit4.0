  function compareDates(t1, t2){
     var one_day=1000*60*60*24; 
     var x=t1.split("/");     
     var y=t2.split("/");
    
     var date1=new Date(x[2],(x[0]-1),x[1]);
     var date2=new Date(y[2],(y[0]-1),y[1])
     
     var month1=x[0]-1;
     var month2=y[0]-1;
    
     _Diff=Math.ceil((date2.getTime()-date1.getTime())/(one_day)); 

     return  _Diff ; 
 }
  
