function calculateArrayPower(){
    moduleTypeQuantity1 = document.getElementById('pg:frm:pb:inputQuantitySPR1');
    moduleTypeQuantity2 = document.getElementById('pg:frm:pb:inputQuantitySPR2');
    
    if (moduleTypeQuantity1.value.replace(/[0-9]/g, '') != '') {
       //.replace(/[0-9]/g, '').replace(/\./, '')
      moduleTypeQuantity1.focus();
    } else if (moduleTypeQuantity2.value.replace(/[0-9]/g, '') != '') {
      moduleTypeQuantity2.focus();
    }
    
    if ((moduleTypeQuantity2.value.replace(/[0-9]/g, '') != '')
        || (moduleTypeQuantity1.value.replace(/[0-9]/g, '') != '')) {
      alert('You must enter a Integer value for Quanity');
      return;
    }
    
    moduleType1 = document.getElementById('pg:frm:pb:inputSPR1').value ;
    moduleType1  = moduleType1.substring(0,3);
    moduleType2 = document.getElementById('pg:frm:pb:inputSPR2').value ;
    moduleType2  = moduleType2.substring(0,3);
    
    ArrayPower = document.getElementById('pg:frm:pb:inputArrayPower');
    ArrayPower.value  = (moduleType1*moduleTypeQuantity1.value) + (moduleType2*moduleTypeQuantity2.value) ;
    
    if(isNaN(ArrayPower.value))
      ArrayPower.value = '' ;
  }


 
