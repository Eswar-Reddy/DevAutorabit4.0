    var moduleTypeQuantitySPR1 = document.getElementById('pg:frm:pb:inputQuantitySPR1');
    var moduleTypeQuantitySPR2 = document.getElementById('pg:frm:pb:inputQuantitySPR2');
    var inverterModel = document.getElementById('pg:frm:pb:inputInverterModel1') ;
    var inverterModelmod = checkInverterModel(inverterModel);
    var SystemSizeValue;
    
    for( var i = 0 ; i < arrayModuleType.length ; i++ ){
