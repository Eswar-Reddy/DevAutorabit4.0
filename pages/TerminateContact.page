<apex:page standardController="Contact" extensions="TerminateContact">
    <apex:pageMessages />
   <apex:form >
    <apex:pageBlock >
      <apex:pageBlockSection >
                <apex:outputLabel value="{!message}" />
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="Proceed" action="{!Proceed}"/>
                <apex:commandButton value="Cancel" action="{!Cancel}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
      </apex:form>
  
     
</apex:page>