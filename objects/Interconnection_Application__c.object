    <validationRules>
        <fullName>App_Passed_Utility_Eng_Review_Not_Future</fullName>
        <active>true</active>
        <description>Application Passed Utility Eng Review date cannot be in the future.</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
Application_Passed_Utility_Eng_Review__c  &gt; today ()</errorConditionFormula>
        <errorDisplayField>Application_Passed_Utility_Eng_Review__c</errorDisplayField>
        <errorMessage>Application Passed Utility Eng Review date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IA_Submitted_to_Utility_Cannot_Be_Older</fullName>
        <active>true</active>
        <description>IA Submitted to Utility date cannot be in the future.</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
IA_Submitted_to_Utility__c &gt; today ()</errorConditionFormula>
        <errorDisplayField>IA_Submitted_to_Utility__c</errorDisplayField>
        <errorMessage>IA Submitted to Utility date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PTO_Date_Cannot_Be_Older_than_today</fullName>
        <active>true</active>
        <description>PTO Date Cannot Be Older than today</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
PTO_Date__c  &gt; today ()</errorConditionFormula>
        <errorDisplayField>PTO_Date__c</errorDisplayField>
        <errorMessage>PTO Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PTO_Date_date_cannot_be_in_the_future</fullName>
        <active>true</active>
        <description>PTO Date date cannot be in the future.</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
(ISNULL(   PTO_Date__c  ) 
 &amp;&amp; 
( PTO_Received__c     = TRUE))</errorConditionFormula>
        <errorDisplayField>PTO_Date__c</errorDisplayField>
        <errorMessage>PTO Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phase_Cannot_Be_Cancelled_and_Completed</fullName>
        <active>true</active>
        <description>Phase Cannot Be Cancelled and Completed</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
(year(Date_Interconnection_Cancelled__c) &gt; 0
&amp;&amp;
(year(Date_Interconnection_Completed__c) &gt; 0))</errorConditionFormula>
        <errorDisplayField>Date_Interconnection_Completed__c</errorDisplayField>
        <errorMessage>Date Cancelled and Completed Cannot Both be Entered - choose one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Inspec_PTO_from_utility_No_Future</fullName>
        <active>true</active>
        <description>Request Inspection &amp; PTO from utility date cannot be in the future.</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
Request_Inspection_PTO_from_utility__c  &gt; today ()</errorConditionFormula>
        <errorDisplayField>Request_Inspection_PTO_from_utility__c</errorDisplayField>
        <errorMessage>Request Inspection &amp; PTO from utility date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Inspection_PTO_Required</fullName>
        <active>true</active>
        <description>Request Inspection &amp; PTO Required</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
(ISNULL(  Request_Inspection_PTO_from_utility__c ) 
 &amp;&amp; 
(Request_Inspection__c = TRUE))</errorConditionFormula>
        <errorDisplayField>Request_Inspection_PTO_from_utility__c</errorDisplayField>
        <errorMessage>Date Inspection was requested is required.</errorMessage>
    </validationRules>
