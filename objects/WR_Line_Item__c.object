    <fields>
        <fullName>Maturity__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( 
IF(UPPER( WR_FDS_Product__r.Product_Type__c) = &apos;INVERTER&apos;,
   10 -  (Year(TODAY()) -  Year(Warranty_Registration__r.Delivery_Date__c)) , 
   IF(UPPER( WR_FDS_Product__r.Product_Type__c) = &apos;MODULE&apos;,
   25  -  (Year(TODAY()) - Year(Warranty_Registration__r.Delivery_Date__c)),
   0)
)
) +&apos; Years left&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Maturity</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
