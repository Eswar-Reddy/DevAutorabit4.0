    <webLinks>
        <fullName>Mail_Warranty_Document</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Email Warranty Document</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}
var selArr = {!GETRECORDIDS($ObjectType.Warranty_Registration__c)};
var conf = false;
if(selArr.length&lt;1)
    alert(&quot;Please select atleast one record.&quot;);
else{
	var lstIds = &apos;&apos;;
	for(var i=0; i&lt;selArr.length; i++)
		lstIds += &apos;\&apos;&apos; + selArr[i] + &apos;\&apos;,&apos;;
	lstIds = &apos;(&apos; + lstIds.substring(0,lstIds.length-1) + &apos;)&apos;;

	var validDes = &apos;(\&apos;warranty certificate\&apos;, \&apos;Warranty Certificate\&apos;, \&apos;WARRANTY CERTIFICATE\&apos;)&apos;;
	//var validCType = &apos;(\&apos;pdf\&apos;,\&apos;PDF\&apos;,\&apos;application/pdf\&apos;)&apos;;
	
	var strQry = &quot;SELECT Id, Name, ContentType, BodyLength, Description FROM Attachment WHERE Description IN &quot; + validDes + &quot; AND ParentId IN &quot; + lstIds; 
	var records = sforce.connection.query(strQry).getArray(&quot;records&quot;);

	if(records.length&gt;0){
		var totSize = 0;
		for(var i=0; i&lt;records.length; i++)
			totSize += parseInt(records[i].BodyLength);
	
		if(totSize==0)
			alert(&apos;No Warranty Certificates files found for selected Warranty registration record(s).&apos;);
		else{
			if(totSize&gt;3140000){
				var tot = Math.round(((totSize/1024)/1024)*100)/100 + &apos; MB&apos;;
				alert(&apos;Total size of attachment file: &apos; + tot + &apos;.\n\nThe total Warranty Certificates file attachment size for selected Warranty registration records is above 3 MB.&apos;);
			}
			else
				window.location=&apos;/apex/WarrentyRegistrationCustom?ids=&apos;+ selArr.join(&apos;-&apos;) + &apos;&amp;retURL=/a1U&apos;;
		}
	}
	else
		alert(&apos;No Warranty Certificates files found for selected Warranty registration record(s).&apos;);
}</url>
