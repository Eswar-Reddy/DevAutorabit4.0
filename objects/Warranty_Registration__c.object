        </filters>
        <filters>
            <field>Delivery_Date__c</field>
            <operation>lessOrEqual</operation>
            <value>2/18/2013</value>
        </filters>
        <filters>
            <field>Partner_SFDC_ID__c</field>
            <operation>contains</operation>
            <value>EnergieRing,alb-elektric Huber,Forstner,Alfred Unterholzner,Ikratos</value>
        </filters>
        <filters>
            <field>Partner_SFDC_ID__c</field>
            <operation>contains</operation>
            <value>bysolar,Wagner,Lichtinger,Wieser,LAN@Work</value>
        </filters>
        <filters>
            <field>Certificate_Status__c</field>
