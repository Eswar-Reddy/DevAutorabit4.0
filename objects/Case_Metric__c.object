        <formula>IF(Duration_In_Hours__c &gt; 0, 
Text(FLOOR(Duration_In_Hours__c/ 24)) &amp; &apos; Days &apos; &amp; 
Text( 
FLOOR( 
(Duration_In_Hours__c- (FLOOR(Duration_In_Hours__c /24) *24)) 
) 
) &amp; &apos; Hours &apos; &amp; 
Text( 
FLOOR( 
(Duration_In_Hours__c- FLOOR(Duration_In_Hours__c)) * 60 
) 
