    <fields>
        <fullName>Sales_Configuration__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Commercial:
Field look up to Sales Configuration 
Facilitates state by state cost breakdown of BOS Costs (Materials,Labor,etc)
PO: Miltiadis Vratimos
PPA Proposal Project: https://www.pivotaltracker.com/epic/show/3273633
Target Release: WW21-24</description>
        <externalId>false</externalId>
        <label>Sales Configuration</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Only PPA Sales Configurations my be attached to a Cost Matrix</errorMessage>
            <filterItems>
                <field>SalesConfiguration__c.Financial_Offering__c</field>
                <operation>equals</operation>
                <value>PPA</value>
            </filterItems>
            <infoMessage>Select the Sales Configuration for the State you wish to configure.
Only PPA Sales Configurations my be attached to a Cost Matrix.</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>SalesConfiguration__c</referenceTo>
        <relationshipLabel>Cost Matrix</relationshipLabel>
        <relationshipName>Cost_Matrix</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>State__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( Sales_Configuration__r.State__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>State</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Technology_Category_Bucket__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
   OR( 
      CONTAINS( TEXT(Technology_System_Category__c), &quot;Helix Roof&quot; ),
      ISPICKVAL( Technology_System_Category__c, &quot;Non-SPWR Roof Mounted Rail&quot; )
   ),
   &quot;Roof&quot;, 
   IF( 
      OR(
        ISPICKVAL( Technology_System_Category__c, &quot;Helix Tracker&quot; ),
        ISPICKVAL( Technology_System_Category__c, &quot;Non-SPWR Ground Fixed Tilt&quot; )
      ),
      &quot;Ground&quot;,
      &quot;Carport&quot;
   )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Technology Category Bucket</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
