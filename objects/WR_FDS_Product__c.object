    <validationRules>
        <fullName>Duplicate_Record_from_Cast_Iron</fullName>
        <active>true</active>
        <description>Duplicate record came for upsert from Cast Iron Process</description>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
NOT(ISNEW()),
NOT(ISChanged( WR_ORACLE_SalesOrder__c )),
NOT(ISChanged( Converted_To_Asset__c )),
NOT(ISChanged(Packing_Slip__c )),
NOT(ISChanged( PickUpDate__c )),
NOT(ISChanged(Alternate_Packing_Slip__c )),
NOT(ISChanged(Serial_Number__c )),
NOT(ISCHANGED( Item__c )),
NOT(ISCHANGED( Product_Name__c )),
NOT(ISCHANGED( Exception_on_ItemID_Populate__c ))
)</errorConditionFormula>
        <errorDisplayField>Third_PL_Name__c</errorDisplayField>
        <errorMessage>Duplicate S/N, PL#,TRD Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pickup_Date_Validation</fullName>
        <active>true</active>
        <description>Don&apos;t allow override TRD_SHIPDATE (Pickup Date) if new date being pushed has older TRD_SHIP_DATE (Pickup Date)</description>
        <errorConditionFormula>IF(  $User.ByPassValidation__c ,false,IF(
       ISNEW(),
        false,
         IF(
         OR(
         AND(
                   NOT(ISNULL( PRIORVALUE( PickUpDate__c) ) ),
                   PickUpDate__c  &lt;   PRIORVALUE( PickUpDate__c)
           ),
          AND(
               ISNULL( PickUpDate__c) ,NOT(ISNULL( PRIORVALUE( PickUpDate__c) ) )
           )
          ),
          true,
          false) 
))</errorConditionFormula>
        <errorDisplayField>PickUpDate__c</errorDisplayField>
        <errorMessage>Pickup Date must not be less than existing date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Registered_Product_Validation</fullName>
        <active>false</active>
        <description>Do not allow to override (update) if converted to asset is marked true</description>
        <errorConditionFormula>IF(
    ISNEW(),
    false,
     (PRIORVALUE(Converted_To_Asset__c)  &amp;&amp; $User.ByPassValidation__c == false) &amp;&amp;       ( NOT(ISBLANK( PRIORVALUE( Item__c))) &amp;&amp;  NOT(ISCHANGED( Item__c ))) == true
)</errorConditionFormula>
        <errorMessage>Registered product can not be updated.</errorMessage>
    </validationRules>
