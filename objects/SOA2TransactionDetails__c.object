        <fullName>Mail_Sent__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(Communication_Sent__c) &amp;&amp;  CreatedDate  &gt;= Partner__r.Subscription_Date__c &amp;&amp; Now() -  CreatedDate  &gt;= 0 &amp;&amp; Now() - CreatedDate &lt;=30, 1,0 )</formula>
