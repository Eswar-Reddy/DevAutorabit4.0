<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Map the lease schedules row to a specific field on the lease schedule summary.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>From_Field__c</fullName>
        <description>This is the API Name of the field that is on the Lease Schedule record.  It will most likely be Annual_Lease_Payment__c for all custom setting records.</description>
        <externalId>false</externalId>
        <inlineHelpText>This is the API Name of the field that is on the Lease Schedule record.  It will most likely be Annual_Lease_Payment__c for all custom setting records, but it always helps to double-check.</inlineHelpText>
        <label>From Field</label>
        <length>100</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>To_Field__c</fullName>
        <description>The API Name of the field on the Lease Schedule Summary object that is the destination of the from field.</description>
        <externalId>false</externalId>
        <inlineHelpText>The API Name of the field on the Lease Schedule Summary object that is the destination of the from field.</inlineHelpText>
        <label>To Field</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Year_Number__c</fullName>
        <description>This is the nth year of the lease schedule.  For example, if there are 20 lease schedule records and we are working with the information for the 18th year, the number in this field should be &quot;18&quot;.</description>
        <externalId>false</externalId>
        <inlineHelpText>This is the nth year of the lease schedule.  For example, if there are 20 lease schedule records and we are working with the information for the 18th year, the number in this field should be &quot;18&quot;.</inlineHelpText>
        <label>Year Number</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Lease Schedule Summary Mapping</label>
    <visibility>Protected</visibility>
</CustomObject>
