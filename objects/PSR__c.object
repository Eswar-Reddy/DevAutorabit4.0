    <validationRules>
        <fullName>Closed_Opportunity_Not_Allowed</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(TEXT(Opportunity__r.StageName), &apos;Closed&apos;)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Please select opportunity which is not closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cost_Estimate_Due_Date</fullName>
        <active>true</active>
        <description>This validation ensures that the Cost Estimate Due date field does not violate the restrictions based on Array Layout Due, Design Tracking, 1L/Electrical Support Due, 1L/Electrical Support Tracking, PSR Sent or CreatedDate</description>
        <errorConditionFormula>AND($User.ByPassValidation__c == false, 
NOT( Business_Unit__c == &apos;NA Commercial&apos;), 
OR(PM_Cost__c,PM_Cost_without_Design__c), 
OR(NOT(( Cost_Estimate_Due__c - CASE( 
MOD( Array_Layout_Due__c - DATE( 1900, 1, 7 ), 7 ), 
2, Array_Layout_Due__c + 2 + 4, 
3, Array_Layout_Due__c + 2 + 4, 
4, Array_Layout_Due__c + 2 + 4, 
5, Array_Layout_Due__c + 2 + 4, 
6, Array_Layout_Due__c + 1 + 4, 
Array_Layout_Due__c + 4 
) ) &gt;= 0), 
NOT(( Cost_Estimate_Due__c - CASE( 
MOD( Electrical_Support_Due__c - DATE( 1900, 1, 7 ), 7 ), 
2, Electrical_Support_Due__c + 2 + 4, 
3, Electrical_Support_Due__c + 2 + 4, 
4, Electrical_Support_Due__c + 2 + 4, 
5, Electrical_Support_Due__c + 2 + 4, 
6, Electrical_Support_Due__c+ 1 + 4, 
Electrical_Support_Due__c + 4 
) ) &gt;= 0), 
NOT(( Cost_Estimate_Due__c - CASE( 
MOD( DATEVALUE(PSR_Sent__c) - DATE( 1900, 1, 7 ), 7 ), 
2, DATEVALUE(PSR_Sent__c) + 2 + 4, 
3, DATEVALUE(PSR_Sent__c) + 2 + 4, 
4, DATEVALUE(PSR_Sent__c) + 2 + 4, 
5, DATEVALUE(PSR_Sent__c) + 2 + 4, 
6, DATEVALUE(PSR_Sent__c) + 1 + 4, 
DATEVALUE(PSR_Sent__c) + 4 
) ) &gt;= 0), 
NOT(( Cost_Estimate_Due__c - CASE( 
MOD( DATEVALUE(CreatedDate) - DATE( 1900, 1, 7 ), 7 ), 
2, DATEVALUE(CreatedDate) + 2 + 4, 
3, DATEVALUE(CreatedDate) + 2 + 4, 
4, DATEVALUE(CreatedDate) + 2 + 4, 
5, DATEVALUE(CreatedDate) + 2 + 4, 
6, DATEVALUE(CreatedDate) + 1 + 4, 
DATEVALUE(CreatedDate) + 4 
) ) &gt;= 0)))</errorConditionFormula>
        <errorDisplayField>Cost_Estimate_Due__c</errorDisplayField>
        <errorMessage>Cost Estimate Due date must be at least 4 weekdays after either Array Layout Due, 1L/Electrical Support Due, PSR Sent or PSR Created Date (whichever is latest)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Electrical_Support_Due_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF($User.ByPassValidation__c == false
,(NOT(( Electrical_Support_Due__c - Array_Layout_Due__c ) &gt;= 1)) &amp;&amp; Single_Line_based_on_Design__c ,null)</errorConditionFormula>
        <errorMessage>Electrical Support Due date must be at least 1 day after Design Due date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NO_PSR_for_SL_Site_Audit_ES_In_Stage_01</fullName>
        <active>false</active>
        <description>Cannot Create PSR for SL, Site Audit, ES when Oppty is &quot;01 â€“ Assess Opportunity&quot;</description>
        <errorConditionFormula>(Business_Unit__c = &quot;NA Commercial&quot;  || Business_Unit__c = &quot;UPP Americas&quot;) 
 &amp;&amp; 
 (Opportunity__r.Stage_Name_Duplicate__c = &quot;01 - Build Single Sales Objective&quot;)
&amp;&amp;
( Site_Survey_Standard__c = True ||
 Array_Layout__c = True ||
 Array_Layout_Revision__c  = True ||
 Single_Line_based_on_Design__c  = True ||
 Single_Line_without_Design__c  = True)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>You cannot create a PSR for Single Line, Site Audit, Electrical Support when Opportunity is in stage &quot;01 - Build Single Sales ObjectiveÂ 
&quot;)</errorMessage>
    </validationRules>
    <validationRules>
