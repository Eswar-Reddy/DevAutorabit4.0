        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Opportunity cannot be in Stage â€™01 â€“ Assess Opportunityâ€™.  Opportunity must have a stage of â€™02 - Budgetary Pricingâ€™ or higher in order to initiate a PSR..</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSR_Cloning</fullName>
        <active>true</active>
        <description>A sent PSR can&apos;t be cloned.</description>
        <errorConditionFormula>IF($User.ByPassValidation__c == false
