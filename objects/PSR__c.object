$User.ProfileId = &quot;00e80000001SSNN&quot;,
$User.ProfileId = &quot;00e80000001SXL2&quot;,
$User.ProfileId = &quot;00e80000001SRp1&quot;,
$User.Id = &quot;005340000089oHh&quot;,
$User.Id = &quot;00580000003YL2q&quot;,
