    <validationRules>
        <fullName>Edit_Is_Not_Allowed</fullName>
        <active>false</active>
        <description>Edit is not allow for non-solution editors</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
( ISPICKVAL( $User.Solutions_Editor__c, &quot;No&quot;)  || ISPICKVAL( $User.Solutions_Editor__c, &quot;&quot;)
&amp;&amp;
NOT(ISPICKVAL( Status, &quot;Draft&quot;) &amp;&amp; NOT(ISPICKVAL( Status, &quot;&quot;)
&amp;&amp;
 (Ischanged ( SolutionName  )  ||    Ischanged ( SolutionNote  ) || Ischanged ( Internal_Content__c  )))))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Only members of the Solutions Steering Committee can update a solution not in Draft Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Solutions_Status_Cannot_Be_Updated</fullName>
        <active>false</active>
        <description>Solutions Status Cannot Be Updated from Null/Draft by non- Solutions Committee Members</description>
        <errorConditionFormula>$User.ByPassValidation__c = False
&amp;&amp;
ISCHANGED(Status)
&amp;&amp;
( ISPICKVAL( $User.Solutions_Editor__c, &quot;No&quot;)  || ISPICKVAL( $User.Solutions_Editor__c, &quot;&quot;)
&amp;&amp;
(ISPICKVAL(PRIORVALUE (Status), &quot;&quot;)  ||  
ISPICKVAL(PRIORVALUE (Status), &quot;Reviewed&quot;)  ||  
ISPICKVAL(PRIORVALUE (Status), &quot;Revised&quot;)  ||  
ISPICKVAL(PRIORVALUE (Status), &quot;Updated&quot;)  ||  
ISPICKVAL(PRIORVALUE (Status), &quot;Pending SME Approval&quot;)  || 
ISPICKVAL(PRIORVALUE (Status), &quot;Pending SSC Approval&quot;)  ||  
ISPICKVAL(PRIORVALUE (Status), &quot;Archived&quot;)))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Only members of the Solutions Steering Committee can update a solution&apos;s status from a Draft Status.</errorMessage>
    </validationRules>
