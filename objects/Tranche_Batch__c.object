&apos;&amp;serverUrl={!API.Partner_Server_URL_80}&amp;id={!Tranche_Batch__c.Id}&amp;QueryId=a2U80000000TNy8&apos;+
&apos;&amp;TemplateId=a2N80000000PB3l&amp;DS7=1&amp;DS7Preview=1&apos;, &apos;_top&apos;);

}

else if( &apos;{!Tranche_Batch__c.Type__c}&apos; == &apos;Presentation (1)&apos; &amp;&amp; &apos;{!Tranche_Batch__c.Financier__c}&apos; == &apos;Credit Suisse&apos; ){

window.open(&apos;https://www.appextremes.com/apps/Conga/PointMerge.aspx?sessionId={!API.Session_ID}&apos;+
&apos;&amp;serverUrl={!API.Partner_Server_URL_80}&amp;id={!Tranche_Batch__c.Id}&amp;QueryId=a2U80000000TNy8&apos;+
&apos;&amp;TemplateId=a2N80000000PB3q&amp;DS7=1&amp;DS7Preview=1&apos;, &apos;_top&apos;);

}

else{
alert(&quot;The Documents are not available for Closing and Payment Tranche Batch and for Financier other than CITI1 and Credit Suisse&quot;);
}</url>
