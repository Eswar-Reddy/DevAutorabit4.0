        <availability>online</availability>
        <description>Case# 00654187 - Service Mgmt Phase 2, First Release (Rel 6.0) - Arokia Nathan</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Case 360 View</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/support/console/36.0/integration.js&quot;)}

function openCase360View() {
	if (sforce &amp;&amp; sforce.console &amp;&amp; sforce.console.isInConsole()) {
		log(&quot;in console&quot;); 
		openCase360Subtab();
	} else {
		log(&quot;not in console&quot;); 
		var loc = &apos;/apex/Case360View?Id=&apos; + &apos;{!WorkOrder.Id}&apos;;
		window.open(loc, &quot;_blank&quot;);
	}
}

function openCase360Subtab() {
	sforce.console.getFocusedPrimaryTabId(openSubtab);
}

var openSubtab = function openSubtab(result) {
	if (result.success == true) {
		var primaryTabId = result.id;
		log(&apos;primaryTabId: &apos; + primaryTabId);
		sforce.console.openSubtab(primaryTabId , &apos;/apex/Case360View?Id={!WorkOrder.Id}&apos;, true, &apos;Case 360 View&apos;, null, openSuccess, &apos;case360ViewSubtab&apos;);
	} else {
		log(&apos;getFocusedPrimaryTabId was not successful: &apos; + result.id);
	}
};

var openSuccess = function openSuccess(result) {
	if (result.success == true) {
		log(&apos;subtab successfully opened&apos;);
	} else {
		log(&apos;subtab cannot be opened&apos;);
	}
};

function log(value) {
	if (window.console &amp;&amp; window.console.log) window.console.log(value);
}

openCase360View();</url>
