    </fields>
    <fields>
        <fullName>Time_to_First_Response__c</fullName>
        <description>Calculates the time (in hours) between the case&apos;s created date and the case timing record created date.</description>
        <externalId>false</externalId>
        <formula>(CreatedDate - Related_Case__r.CreatedDate) * 24</formula>
        <label>Time to First Response (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Time_Spent__c</fullName>
        <description>This field calculates the difference between the Started Work and Work Ended timings to show the total time spent working on a case.</description>
        <externalId>false</externalId>
        <formula>(Work_Ended__c - Started_Work__c) * 24</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Time Spent (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
