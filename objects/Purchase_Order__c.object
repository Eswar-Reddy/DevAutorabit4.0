    <webLinks>
        <fullName>Clone</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clone</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var contactId = &quot;{!User.ContactId}&quot;;
var profileName = &quot;{!$Profile.Name}&quot;;

if (contactId == &quot;&quot; || profileName == &quot;Partner Restricted&quot; || profileName == &quot;EU Partner Restricted&quot;) {
  alert(&quot;You do not have access to Clone Purchase orders.&quot;);
} else {
  var arrayOfRecords = {!GETRECORDIDS($ObjectType.Purchase_Order__c)};
  
  if (arrayOfRecords.length == 0) {
    alert(&quot;You must select one record to clone&quot;);
  } else if (arrayOfRecords.length &gt; 1) {
    alert(&quot;Please select only one record to clone. Clone can not be done on multiple records&quot;);
  } else {
    window.top.location.href = &quot;/apex/Order_Clone?clone=1&amp;id=&quot; + arrayOfRecords[0];
  }
}</url>
    </webLinks>
