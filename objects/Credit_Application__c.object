    <webLinks>
        <fullName>Submit</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)} 

var creAppStatus = &quot;{!Credit_Application__c.Status__c}&quot;; 
var CANumber =  &quot;{!Credit_Application__c.Name}&quot;;

if (creAppStatus == &quot;Ready for Review&quot;) { 
alert(&quot;This CA has already been submitted.&quot;); 
} else if (creAppStatus != &quot;Approved&quot; &amp;&amp; creAppStatus != &quot;Closed&quot; &amp;&amp; creAppStatus != &quot;Declined&quot; 
&amp;&amp; creAppStatus != &quot;In Progress&quot; &amp;&amp; creAppStatus != &quot;Withdrawn&quot;) { 
var termsCheck = &quot;{!Credit_Application__c.Terms_and_Conditions__c}&quot;; 
if(termsCheck == false &amp;&amp; &quot;{!Credit_Application__c.RecordType}&quot; ==&quot;EU Credit Application&quot;){ 
alert(&quot;{!$Label.CASubmitTnCError}&quot;); 
}else{ 
var error = sforce.apex.execute(&quot;CreditApplicationSubmit&quot;, &quot;submit&quot;, {creAppId:&apos;{!Credit_Application__c.Id}&apos;}); 
if(error!=&apos;&apos;){ 
alert(error); 
}else{
alert(&quot;{!$Label.CASubmitSuccessMessage1} &quot; + CANumber + &quot; {!$Label.CASubmitSuccessMessage2}&quot;); 
window.parent.location.reload();} 
} 
} else { 
alert(&quot;This CA cannot be submitted.&quot;); 
}</url>
    </webLinks>
