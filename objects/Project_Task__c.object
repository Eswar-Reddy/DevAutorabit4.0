    <fields>
        <fullName>SLA_Days_Aging__c</fullName>
        <description>Project Name: Residential Installation Management 
Requester: Priti Rastogi 
SLA clock for task completion.</description>
        <externalId>false</externalId>
        <formula>IF(Status_Type__c = &apos;Open&apos;, 
   now() - Elapsed_DateTime__c + Time_Differential__c, 
   IF(Status_Type__c = &apos;Hold&apos;, 
       Time_Differential__c, 
       IF(Status_Type__c = &apos;Completed&apos;,
           End_DateTime__c - Elapsed_DateTime__c +  Time_Differential__c , 
           NULL)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SLA Days Aging</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
