    <validationRules>
        <fullName>Active_to_Not_Started_Validation</fullName>
        <active>true</active>
        <description>Once a task has been started, it cannot go back into a &apos;Not Started&apos; status.</description>
        <errorConditionFormula>AND( $User.ByPassValidation__c = FALSE, Status_Type__c == &apos;New&apos; , Priorvalue(Status_Type__c) != &apos;New&apos;, Apex_Updated__c == PRIORVALUE(Apex_Updated__c))</errorConditionFormula>
        <errorMessage>Once a task is started it cannot go back into &apos;Not Started&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_to_In_Progress_Validation</fullName>
        <active>true</active>
        <description>Once a task is completed it cannot go back into a &apos;In Progress&apos; status.</description>
        <errorConditionFormula>AND( $User.ByPassValidation__c = FALSE, TEXT(Status__c) = &apos;In Progress&apos; , ISPICKVAL(PRIORVALUE(Status__c), &apos;Completed&apos;))</errorConditionFormula>
        <errorMessage>Once a task is &apos;Completed&apos; it cannot go back to &apos;In Progress&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Critical_Path_Order_Within_Range</fullName>
        <active>true</active>
        <description>The Order of a critical path task must not be below 1 or above the highest existing order number +1</description>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Task_Template&apos;, Critical_Path__c = TRUE, OR(Order__c &lt; 1, Order__c &gt; (Residential_Project__r.Max_Order__c +1)))</errorConditionFormula>
        <errorMessage>The order of this task is invalid. It must not be lower than 1 or above the highest existing order +1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Critical_Path_and_Order</fullName>
        <active>true</active>
        <description>The critical path checkbox cannot be changed in the same action as order</description>
        <errorConditionFormula>AND(PRIORVALUE(Critical_Path__c) != Critical_Path__c, PRIORVALUE(Order__c) != Order__c)</errorConditionFormula>
        <errorMessage>Critical Path and Order cannot be changed in the same action. Please update one at a time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Critical_Path_on_Template_Only</fullName>
        <active>false</active>
        <description>The critical path can be set on templates only</description>
        <errorConditionFormula>AND(Critical_Path__c = TRUE,  Template_Task__c = NULL,  RecordType.Name != &apos;Task Template&apos;)</errorConditionFormula>
        <errorMessage>Critical Path can only be set at the template level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Critical_Path_without_Order</fullName>
        <active>true</active>
        <description>All critical path tasks must have and order assigned to them.</description>
        <errorConditionFormula>AND(Critical_Path__c = TRUE, OR(ISNULL(Order__c), Order__c = 0))</errorConditionFormula>
        <errorMessage>All critical path tasks must have an order assigned to them</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Homeowner_Scheduled_Installation_Date</fullName>
        <active>false</active>
        <description>Validates the Homeowner schedule installation date field on the project is populated before completing the supporting task</description>
        <errorConditionFormula>AND(Name = &apos;Schedule Install with Homeowner&apos;,  ISNULL(Residential_Project__r.Homeowner_Scheduled_Installation_Date__c), Status_Type__c = &apos;Completed&apos;)</errorConditionFormula>
        <errorMessage>The Homeowner Scheduled Installation Date field on the project must be populated before completing this task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>In_Progress_to_Completed_Validation</fullName>
        <active>true</active>
        <description>A task must be in an &apos;In Progress&apos; status to be moved to Completed</description>
        <errorConditionFormula>AND( $User.ByPassValidation__c = FALSE, ISPICKVAL(Status__c, &apos;Completed&apos;), PRIORVALUE(Status_Type__c) != &apos;Open&apos;, ISCHANGED(Status__c))</errorConditionFormula>
        <errorMessage>A task must be In Progress before it moves to Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Needs_Rework_to_In_Progress_Validation</fullName>
        <active>true</active>
        <description>A task cannot go from a &apos;Needs Rework&apos; status to an &apos;In Progress&apos; status</description>
        <errorConditionFormula>AND( $User.ByPassValidation__c = FALSE, ISPICKVAL(Status__c, &apos;In Progress&apos;), ISPICKVAL(PRIORVALUE(Status__c), &apos;Needs Rework&apos;))</errorConditionFormula>
        <errorMessage>A task cannot go from &apos;Needs Rework&apos; to &apos;In Progress&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pending_Rework_to_In_Progress</fullName>
        <active>true</active>
        <description>A task can only go from Pending Rework to In Progress automatically. It can not be manually changed.</description>
        <errorConditionFormula>AND(NOT(ISCHANGED(Apex_Updated__c)),  $User.ByPassValidation__c = FALSE, TEXT(Status__c) = &apos;In Progress&apos; , ISPICKVAL(PRIORVALUE(Status__c), &apos;Pending Rework&apos;))</errorConditionFormula>
        <errorMessage>Please Complete the Task Rework Request to change the status to &apos;In Progress&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Task_Assignment</fullName>
        <active>true</active>
        <description>If a task is in progress or pending approval an assigned to user is required</description>
        <errorConditionFormula>AND(OR(TEXT(Status__c) = &apos;In Progress&apos;,TEXT(Status__c) = &apos;Pending Approval&apos;) , Assigned_To__c = NULL)</errorConditionFormula>
        <errorMessage>Assigned To User is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_to_Operation_Validation</fullName>
        <active>false</active>
        <description>Validates that all EC required fields on the Project are populated before the Sales to Operation task can be completed</description>
        <errorConditionFormula>AND(Name = &apos;Sales to Operations Handoff&apos;, 
Status_Type__c = &apos;Completed&apos;, OR(
 ISPICKVAL(Residential_Project__r.HOA__c, &apos;&apos;),
 ISBLANK(Residential_Project__r.Homeowner_Utility_Account_No__c),  ISBLANK(Residential_Project__r.Homeowner_Utility_Meter_No__c),  ISPICKVAL(Residential_Project__r.Main_Breaker_Location__c,&apos;&apos;), ISPICKVAL(Residential_Project__r.Main_Breaker_Rating_Amps__c,&apos;&apos;), ISPICKVAL(Residential_Project__r.Main_Service_Panel_Location__c,&apos;&apos;),ISPICKVAL(Residential_Project__r.Main_Service_Panel_Manufacturer__c,&apos;&apos;),ISPICKVAL(Residential_Project__r.Smart_Thermostat__c,&apos;&apos;)))</errorConditionFormula>
        <errorMessage>To complete this task, please populate the following fields. HOA, Homeowner Utility Account No, Homeowner Utility Meter No, Main Breaker Location, Main Breaker Rating(Amps), Main Service Panel Location, Main Service Panel Manufacturer, Smart Thermostat</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_AHJ_Inspection</fullName>
        <active>false</active>
        <description>Validates the Scheudle AHJ Inspection date field on the project is populated before completing the supporting task</description>
        <errorConditionFormula>AND(Name = &apos;Schedule Inspection&apos;,  ISNULL( Residential_Project__r.Scheduled_AHJ_Inspection__c ), Status_Type__c = &apos;Completed&apos;)</errorConditionFormula>
        <errorMessage>The Schedule AHJ Inspection field on the project must be populated before completing this task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_Site_Survey_Date_Validation</fullName>
        <active>false</active>
        <description>Validate the Site Survey Date field on the Project is populated before the Homeowner Welcome Call or Schedule Site Survey tasks can be completed</description>
        <errorConditionFormula>AND(Name = &apos;Homeowner Welcome Call&apos; , ISNULL( Residential_Project__r.Scheduled_Site_Survey_Date__c ), Status_Type__c = &apos;Completed&apos;)</errorConditionFormula>
        <errorMessage>The Scheduled Site Survey Date field on the Project must be populated before completing this task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Progression_Restriction</fullName>
        <active>true</active>
        <description>Only tasks next in line can be started.</description>
        <errorConditionFormula>AND(NOT(ISCHANGED(Apex_Updated__c)),IF(ISNULL(Residential_Project__r.Last_Completed_Step__c), AND(Critical_Path__c = TRUE,ISPICKVAL(PRIORVALUE(Status__c), &apos;Not Started&apos;), ISPICKVAL(Status__c, &apos;In Progress&apos;), Order__c &gt; 1),  AND(Critical_Path__c = TRUE, ISPICKVAL(PRIORVALUE(Status__c), &apos;Not Started&apos;), ISPICKVAL(Status__c, &apos;In Progress&apos;), Order__c &gt; (Residential_Project__r.Last_Completed_Step__c +1))))</errorConditionFormula>
        <errorMessage>Only tasks next in order can become &apos;In Progress&apos;</errorMessage>
    </validationRules>
