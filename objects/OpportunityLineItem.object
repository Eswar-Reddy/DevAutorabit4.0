    <validationRules>
        <fullName>Cost_Restriction</fullName>
        <active>true</active>
        <description>Cost Restriction.</description>
        <errorConditionFormula>(ISCHANGED(  Cost__c )
 &amp;&amp; 
  CONTAINS(Opportunity.Pricebook2.Name, &apos;Component&apos;)
  &amp;&amp;
   Cost__c  &lt;&gt;  PricebookEntry.Product2.Cost__c)</errorConditionFormula>
        <errorDisplayField>Cost__c</errorDisplayField>
        <errorMessage>Cost Cannot be Updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Cost_on_Total_Price_Line_Item</fullName>
        <active>true</active>
        <description>Total Price line items should not have cost populated.</description>
        <errorConditionFormula>IF( Contains( Opportunity.Pricebook2.Description, &quot;System&quot;),
AND(
ISPICKVAL(Product2.Family , &quot;Total Price&quot;), Cost__c &gt;0), false)</errorConditionFormula>
        <errorDisplayField>Cost__c</errorDisplayField>
        <errorMessage>The Total Price line item should only contain sales price, cost should not be populated. Please enter a 0 value for cost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Price_on_Product_Line_Item</fullName>
        <active>true</active>
        <description>Product line items should not have sales price populated.</description>
        <errorConditionFormula>IF( Contains( Opportunity.Pricebook2.Description, &quot;No Price in Product&quot;),
AND(
NOT(ISPICKVAL(Product2.Family , &quot;Total Price&quot;)), UnitPrice &gt;0,
NOT(ISPICKVAL(Product2.Family, &quot;Freight&quot;)),
NOT(ISPICKVAL(Product2.Family , &quot;O&amp;M&quot;))), false)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sales Price should not be populated for individual product line items. Please put 0 as the Sales Price for this product, add a Total Price product line item to capture the total amount of the deal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Locked_After_100</fullName>
        <active>true</active>
        <description>Opportunity is Locked After is 100%. Product cannot be edited.</description>
        <errorConditionFormula>Opportunity.Probability = 1 
&amp;&amp; 
NOT($Profile.Id = &apos;00e80000001SRp5&apos;) 
&amp;&amp; 
NOT($Profile.Id = &apos;00e80000000tepU&apos;) 
&amp;&amp; 
NOT($Profile.Id = &apos;00e80000001SS4Q&apos;)</errorConditionFormula>
        <errorMessage>Once an opportunity reaches 100%, product cannot be edited. Please see your SalesForce administrator for assistance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_must_equal_1_if_multiplier_is_1</fullName>
        <active>true</active>
        <description>For any product that has a multiplier as 1, the user must enter a quantity of 1.</description>
        <errorConditionFormula>IF( Contains( Opportunity.Pricebook2.Description, &quot;System&quot;),
AND( Product2.Unit__c &lt;=1, Quantity &gt;1 &amp;&amp; PricebookEntry.Product2.Product_Family_Text__c &lt;&gt; &quot;Inverters&quot;) &amp;&amp; PricebookEntry.Product2.Product_Family_Text__c &lt;&gt; &quot;Freight&quot;  &amp;&amp; PricebookEntry.Product2.Product_Family_Text__c &lt;&gt; &quot;Storage&quot; &amp;&amp; PricebookEntry.Product2.Product_Family_Text__c &lt;&gt; &quot;Development&quot;,
false)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>Quantity for this product must equal 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Price_Restriction</fullName>
        <active>true</active>
        <description>Sales Price Restriction.</description>
        <errorConditionFormula>(ISCHANGED( UnitPrice )
 &amp;&amp; 
  (CONTAINS(Opportunity.Pricebook2.Name, &apos;Component&apos;)  || 
CONTAINS(Opportunity.Pricebook2.Name, &apos;Cost by Quarter&apos;))&amp;&amp;
NOT(CONTAINS(Opportunity.Pricebook2.Name, &apos;Component&apos;)))</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Change Price back to previous value or cancel. Enter Price At the Sales Unit Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Systems_Opportunity_use_Qty_for_kWp</fullName>
        <active>true</active>
        <description>This is to validate that if a user is dealing with a Systems type opportunity, the Quantity field should be used for kWp, not the kWp field used for Components deals.</description>
        <errorConditionFormula>IF( Contains( Opportunity.Pricebook2.Description, &quot;System&quot;),
kWp_Components__c &gt; 0, false)</errorConditionFormula>
        <errorMessage>You must zero out the kWp field and enter kWp in the Quantity field.</errorMessage>
    </validationRules>
</CustomObject>
