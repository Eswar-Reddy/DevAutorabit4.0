    </validationRules>
    <validationRules>
        <fullName>Roof_Height_Mandatory</fullName>
        <active>true</active>
        <description>to check the mandatory field for roof height</description>
        <errorConditionFormula>and(if(ISNULL(Roof_Height_M_FR__c) , True ,False ), Account__r.BillingCountry = &apos;France&apos;)</errorConditionFormula>
        <errorMessage>Hauteur du toit: Vous devez saisir une valeur</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Roof_Height_Validation</fullName>
        <active>true</active>
        <description>Roof Height should be between 2m and 8m</description>
        <errorConditionFormula>and (If (Roof_Height_M_FR__c &lt; 2  || Roof_Height_M_FR__c &gt; 8, True ,False ), Account__r.BillingCountry = &apos;France&apos;)</errorConditionFormula>
        <errorMessage>Hauteur de toit que vous avez entrรฉ n&apos;est pas dans autorisรฉes range.Roof Hauteur limites admissibles est comprise entre 2 m et 8m.Please re entrer la hauteur du toit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shading_Measurement_Date_validation</fullName>
        <active>true</active>
        <description>Shading measurement date should not accept future dates</description>
        <errorConditionFormula>AND(Shading_Measurement_Date__c  &gt;  TODAY(), Account__r.BillingCountry =&apos;France&apos;)</errorConditionFormula>
        <errorDisplayField>Shading_Measurement_Date__c</errorDisplayField>
        <errorMessage>Veuillez saisir une date valide. Ne peut pas รชtre une date future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Solar_Access_Month_Validation</fullName>
        <active>true</active>
        <description>Solar Access Month Validation</description>
        <errorConditionFormula>and(If (Jan__c &lt; 0 || Jan__c &gt; 100,true, If(Feb__c &lt; 0 || Feb__c &gt; 100,true,If(Mar__c &lt; 0 || Mar__c &gt; 100,true,if(Apr__c &lt; 0 || Apr__c &gt; 100,true,if(May__c &lt; 0 || May__c &gt; 100,true, if(Jun__c &lt; 0 || Jun__c &gt; 100,true, if(Jul__c &lt; 0 || Jul__c &gt; 100,true,if(Aug__c &lt; 0 || Aug__c &gt; 100,true, if(Sep__c &lt; 0 || Sep__c &gt; 100,true, if(Oct__c &lt; 0 || Oct__c &gt; 100,true,if(Nov__c &lt; 0 || Nov__c &gt; 100,true,if(Dec__c &lt; 0 || Dec__c &gt; 100, true , false) ))))))))))),Account__r.BillingCountry = &apos;France&apos;)</errorConditionFormula>
        <errorMessage>Accรจs solaire valeur du mois saisi n&apos;est pas dans les limites permises range.Allowable est comprise entre 0 et 100.Please re entrer.</errorMessage>
    </validationRules>
