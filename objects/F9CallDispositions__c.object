<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>List containing call dispositions used by Five9, referenced when updating Lead Status from call results.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Consultation_Type__c</fullName>
        <externalId>false</externalId>
        <label>Consultation Type</label>
        <length>200</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Disposition__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Field to store the call disposition (CallResult) of the Five9 task/activity history. MUST BE UNIQUE as a key for map to corresponding values.</description>
        <externalId>false</externalId>
        <label>Disposition</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Lead_Status__c</fullName>
        <description>Holds Corresponding lead status for each call disposition</description>
        <externalId>false</externalId>
        <label>Lead Status</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Reason__c</fullName>
        <externalId>false</externalId>
        <label>Reason</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Trigger_Eloqua_Follow_Up__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Used to map to checkbox field that Eloqua can read to send follow-up information such as postcards and other marketing material.</description>
        <externalId>false</externalId>
        <label>Trigger Eloqua Follow-Up</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Trigger_No_Contact_Activities__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Used to map to checkbox field that Eloqua can read to proceed with no-contact activities</description>
        <externalId>false</externalId>
        <label>Trigger No-Contact Activities</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>F9 Call Dispositions</label>
    <visibility>Public</visibility>
</CustomObject>
