<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>This custom setting is used to set the SLAs for Problem CRM Cases. Numbers are written in Days.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Time_to_Assignment__c</fullName>
        <description>This field sets the SLA for CRM Case Assignment (Owner changed from CRM Case to a user.)</description>
        <externalId>false</externalId>
        <label>Time to Assignment</label>
        <precision>10</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_to_Close__c</fullName>
        <description>This field sets the SLA for CRM Cases time to closure. Case opened - Case Closed .</description>
        <externalId>false</externalId>
        <label>Time to Close</label>
        <precision>10</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_to_Resolution__c</fullName>
        <description>This field sets the SLA for time to resolution. (Solution Provided)</description>
        <externalId>false</externalId>
        <label>Time to Resolution</label>
        <precision>10</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_to_Response__c</fullName>
        <description>This field sets the SLA for time to response.</description>
        <externalId>false</externalId>
        <label>Time to Response</label>
        <precision>10</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_to_Triage__c</fullName>
        <description>This field sets the SLA for time to triage.</description>
        <externalId>false</externalId>
        <label>Time to Triage</label>
        <precision>10</precision>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
