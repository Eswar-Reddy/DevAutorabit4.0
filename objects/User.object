    <validationRules>
        <fullName>Internal_User_Required_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.ByPassValidation__c )&amp;&amp;

AND( 
NOT(ISPICKVAL( UserType,&quot;PowerPartner&quot;)), 
OR(LEN(Country)=0, LEN( Division )=0) 
)</errorConditionFormula>
        <errorMessage>An internal user must have a division and country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Who_Can_Activate_Users</fullName>
        <active>true</active>
        <description>Limit Who Can Activate Users</description>
        <errorConditionFormula>(( ISCHANGED(IsActive )
&amp;&amp; 
  $User.Edit_Partner_Users__c  = False
&amp;&amp;
( Not(CONTAINS( Role_text__c, &apos;Partner User&apos;)  || 
  Not(CONTAINS(Role_text__c, &apos;Partner Manager&apos;)  || 
 Not(CONTAINS(Role_text__c, &apos;Partner Executive&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Sales&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Delegated Administrator&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Operations&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Restricted&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Operations&apos;) || 
 Not(CONTAINS(Role_text__c, &apos;Partner Technical&apos;) 
))))))))))))</errorConditionFormula>
        <errorMessage>Please contact your system administrator to activate user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_on_Who_Can_Activate_Portal_Users</fullName>
        <active>true</active>
        <description>Limit on Who Can Activate Portal Users</description>
        <errorConditionFormula>( $User.ByPassValidation__c = False &amp;&amp; ( ISCHANGED(IsActive)
&amp;&amp;
  ($User.Edit_Partner_Users__c = False)
 &amp;&amp; 
( (    CONTAINS( Role_text__c, &apos;Partner User&apos;)  || 
 (    CONTAINS( Role_text__c, &apos;Partner Manager&apos;)  || 
 (    CONTAINS( Role_text__c, &apos;Partner Executive&apos;) 
))))))</errorConditionFormula>
        <errorMessage>Please contact partnerportal@sunpowercorp.com to have a new Partner user activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_on_Who_Can_Create_Portal_Users</fullName>
        <active>true</active>
        <description>Limit on Who Can Create Portal Users</description>
        <errorConditionFormula>ISNEW()
&amp;&amp;
($User.ByPassValidation__c = False 
&amp;&amp;
  ($User.Edit_Partner_Users__c = False))
&amp;&amp;
(( CONTAINS( Role_text__c, &apos;Partner User&apos;) || 
( CONTAINS( Role_text__c, &apos;Partner Manager&apos;) || 
( CONTAINS( Role_text__c, &apos;Partner Executive&apos;) 
))))</errorConditionFormula>
        <errorMessage>Please contact partnerportal@sunpowercorp.com to have a new Partner user activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_on_Who_Can_Create_Standard_Users</fullName>
        <active>true</active>
        <description>Pivotal# 145793091 Add validation rule for system admin</description>
        <errorConditionFormula>AND (
  ISNEW(),
  $User.Profile_Text__c != &apos;System Administrator&apos;,
  ISPICKVAL(UserType, &apos;Standard&apos;)
)</errorConditionFormula>
        <errorMessage>Please contact system administrator to create standard user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partner_TenantId_Required</fullName>
        <active>true</active>
        <description>This validation rule is to make sure a Partner User is not given Aurora Design Licensee permission unless Partner Account has Tenant ID populated</description>
        <errorConditionFormula>AND( NOT(ISPICKVAL(UserType, &apos;Standard&apos;)), INCLUDES( Business_Function__c,&apos;Aurora Design Licensee&apos;), 
OR(ISNULL(Contact.Account.Design_Tool_Organization_Id__c),
ISBLANK(Contact.Account.Design_Tool_Organization_Id__c)) )</errorConditionFormula>
        <errorDisplayField>Business_Function__c</errorDisplayField>
        <errorMessage>Cannot assign &apos;Aurora Design Licensee&apos;  unless Partner Account has a valid Tenant Id</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Design</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(ISPICKVAL(UserType, &apos;Standard&apos;)), 
($User.USERID_18character__c &lt;&gt; &quot;00580000003aECRAA2&quot;), 
INCLUDES( Business_Function__c,&apos;Request Design&apos;), 
NOT(ISBLANK(Contact.Account.Design_Tool_Organization_Id__c)), 
NOT( OR( 
CONTAINS( Contact.Account_Type__c , &quot;Commercial&quot;), 
CONTAINS( Contact.Account_Type__c , &quot;Combo&quot;) 
)) 
)</errorConditionFormula>
        <errorDisplayField>Business_Function__c</errorDisplayField>
        <errorMessage>Request Design is not an option since Partner has Aurora Design License</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SOX_Compliance_Profile_Added_to_New_User</fullName>
        <active>true</active>
        <description>Checks whether a new user is assigned to a SOX regulated profile and whether the &quot;SOX Profile Approval Received&quot; checkbox is checked. - John Taylor 10/30/2014</description>
        <errorConditionFormula>AND(
$User.ByPassValidation__c == false,
ISNEW(),   
(OR(Profile.Name = &quot;SunPower Lease Payment Access&quot;, 
Profile.Name = &quot;SunPower Lease/Pricing Administrator&quot;, 
Profile.Name = &quot;Global Sales Ops&quot;, 
Profile.Name = &quot;SunPower Order Management&quot;, 
Profile.Name = &quot;Sunpower Pricing Administrator&quot;,
Profile.Name = &quot;System Administrator&quot;)),
SOX_Profile_was_approval_received__c == false)</errorConditionFormula>
        <errorDisplayField>SOX_Profile_was_approval_received__c</errorDisplayField>
        <errorMessage>Have you received approval to assign this new user this profile that is governed by SOX Compliance?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SOX_Compliance_Profile_Added_to_User</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ByPassValidation__c), 
(SOX_Profile_was_approval_received__c = False),
(ISCHANGED(  ProfileId  )),
(PRIORVALUE(ProfileId) &lt;&gt; ProfileId),
(OR(Profile.Name = &quot;SunPower Lease Payment Access&quot;,
Profile.Name = &quot;SunPower Lease/Pricing Administrator&quot;,
Profile.Name = &quot;Global Sales Ops&quot;,
Profile.Name = &quot;SunPower Order Management&quot;,
Profile.Name = &quot;Sunpower Pricing Administrator&quot;,
Profile.Name = &quot;System Administrator&quot;)))</errorConditionFormula>
        <errorDisplayField>SOX_Profile_was_approval_received__c</errorDisplayField>
        <errorMessage>Have you received approval to assign this profile that is governed by SOX Compliance?</errorMessage>
    </validationRules>
